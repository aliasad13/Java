package inheritance;

class Add{
	
	public int addition(int num1,int num2)
	{
	return num1+num2;	
		
	}
}

class AddSub{
	public int addition(int num1,int num2)
	{
	return num1+num2;	
		
	}
	
	public int sub(int num1,int num2)
	{
		return num1-num2;
		
	}
	
	
	
}

public class Inheritance {

	public static void main(String[] args) {
			
		AddSub obj=new AddSub();
		
	    System.out.println(obj.addition(5,4 ));
	    
	
	}
	
	

}
