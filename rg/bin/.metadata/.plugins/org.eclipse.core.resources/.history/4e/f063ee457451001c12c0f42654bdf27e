package patterns;
/*
 *  *
 *  **
 *  ***
 *  ****
 * 
 */
public class Pattern2 {

	public static void main(String[] args) {
		
		int c=0;
		for (char i =1;i<=4;i++) {
			
			for (char j =1;j<=i;j++) {
				
				System.out.print(c);
				c++;
				
				}
			System.out.println();
			
		} 
		
		

	}

}
