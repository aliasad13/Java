package patterns;
/*
 *  *
 *  **
 *  ***
 *  ****
 * 
 */
public class Pattern2 {

	public static void main(String[] args) {
		
		for (char i =1;i<=4;i++) {
			
			for (char j =1;j<=i;j++) {
				
				System.out.print((char)(j+64));
				
				}
			System.out.println();
			
		} 
		
		

	}

}
