package patterns;

/*
 *      *
 *     **     
 *    ***
 *   ****
 *  *****   
 */
     

public class Pattern6 {

	public static void main(String[] args) {
		for (char i=1;i<=5;i++){
			
			
			for (char j=5;j>=i;j--)
			{
				System.out.print(" ");
			}
			
			for (char j=1;j<=i;j++){
				System.out.print((char)(i+64)+" ");
			}
			
			
			System.out.println();
			}
		}
		
		
		
		
		
	}


