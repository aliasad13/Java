package com.armstrong;


/*
 * 153=1*1*1  +  5*5*5  + 3*3*3
 */

public class Armstrong {

	public static void main(String[] args) {
		
		int sum=0;
		int n=153;
		int temp=n;
		
		while (n>0) {
		int r =n%10;
		 n=n/10;
		
		 sum = sum + r*r*r;
		 
		 System.out.println(sum);
		}
		if (temp == sum)
			System.out.println("yes");
		else {
			System.out.println("no");
		}
	}

}
