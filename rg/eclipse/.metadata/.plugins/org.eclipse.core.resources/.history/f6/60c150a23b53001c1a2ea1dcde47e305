package anonymous;

class A {
	
	public void show() {
		
		System.out.println("in A");
	}}
	
	
	public class Anonymous2{


	public static void main(String[] args) {

		A obj = new A(){
			public void show() {
				System.out.println("in B");
			}
			
		};
		
		obj.show();

		

	}

}
