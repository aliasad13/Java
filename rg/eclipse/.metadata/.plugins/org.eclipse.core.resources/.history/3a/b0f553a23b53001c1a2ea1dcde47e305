package assnmnts;
/*
 * A
 * BB
 * CCC
 * DDDD
 * EEEEE
 */

public class Assignment4 {

	public static void main(String[] args) {
		char i,j;
		for (i=1;i<=5;i++)
		{
			for (j=1;j<=i;j++)
			{
				System.out.print((char)(i + 64));
				
			}
		System.out.println();	
		}

	}

}
