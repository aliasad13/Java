package objectMethodConstructor;//TO SHOW ANOTHER WAY TO FIND OUTPUT IN CONSTRUCTOR

class A{
	public A(double i,double j,double k)
	{
		System.out.println(i+j+k);
		
	}	
	
	public A(int i,int j,int k)
	{
		System.out.println(i+j+k);
		
	}	
	
	
}

public class Constructor2 {

	public static void main(String[] args) {
	A obj = new A(5.5,6.6,7.6);
	
	
	}

}
