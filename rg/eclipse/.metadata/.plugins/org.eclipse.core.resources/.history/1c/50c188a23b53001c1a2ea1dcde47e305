package operators;

public class TernaryOperator {
	
	public static void main(String[] args)
	{
		int i=4,j=6;
		if(i>5)
			j=1;
			else
				j=2;
		System.out.println("j is "+ j);}
	
	public static void easyWay(String[] args)
	{int i=4,k=6;
		k =i>5?1:2;
		System.out.println("k is"+ k);
		}
	}
	


