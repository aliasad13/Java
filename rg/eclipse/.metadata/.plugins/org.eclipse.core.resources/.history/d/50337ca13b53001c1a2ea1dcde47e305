package functinalinterfaceAndlambdaexpression;

interface Abc{
	
void show();
}
public class FunctionalInterface {

	public static void main(String[] args) {              
		
        Abc obj = () ->{
        		
        	System.out.println("show"); 
        	
        };

obj.show();
              
	}}


