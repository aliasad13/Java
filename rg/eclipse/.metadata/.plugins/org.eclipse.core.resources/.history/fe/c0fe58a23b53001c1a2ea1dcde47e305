package finalkeyword;  //error is required to learn final keyword
class Variable{
	
	final int i=0;
	
	public Variable() {        // if we had used method here instead of constructor, the value of i wont change,i=0
		                       //even without final keyword
	 i =5;
		
	}
	
}

public class FinalVariable {

	public static void main(String[] args) {
		Variable obj = new Variable();
		System.out.println(obj.i);
	}

}
