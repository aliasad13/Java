package re;

class A{
	
	public void show() {
		System.out.println("hello from A");
	}	
}

class B extends A{
	
	public void show() {
		System.out.println("hello from b ");
	}	
}



public class Rere {

	public static void main(String[] args) {
	
		B obj = new B();
		obj.show();
		
		
	}

}
