package re;

interface A{
	
	public abstract void show();	
}






public class Rere {

	public static void main(String[] args) {
	
		A obj = new A()
				{
			public void show() {
				System.out.println("hello from b");  //anaonymous class
			}	
			
			
				}
				;
		obj.show();
		
		
	}

}
