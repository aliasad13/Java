package oddoreven;

public class OddOrEvev {

	public static void main(String[] args) {
		for (int i=1;i<=100;i++)
		{
			
			if(i%2==0) {
				System.out.print(i+" ");
			for (int row=1;row<=5;row++) {
				for (int col=1;col<=i;col++) {
					System.out.print(i);
				}
				System.out.println();
			}
			}
			
		}

	}

}
