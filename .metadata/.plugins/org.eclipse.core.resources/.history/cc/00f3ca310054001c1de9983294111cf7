package revision;

class A{
	int res;
	public void perform(int i,int j) {
	
		
		res=i+j;
		System.out.println(res);
	}
	public int show(int a,int b) {
		return a+b;
	}
}

public class Revision {

	public static void main(String[] args) {
		
				A obj =new A();
			obj.perform(5, 4);
			System.out.println(obj.show(5, 4));
	}

}
