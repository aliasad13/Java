package practice;

public class Try {

	public static void main(String[] args) {
		
		char i,j;
		
		for (i = 1;i<=5;i++)
		{
			
			for (j=1;j<=5;j++)
			{
				if (i==1||j==1|i==5|j==5) {
				
				System.out.print((char)(j+64));}
				
				else {
					System.out.print(" ");
				}
				
				
			}System.out.println();
		}
		

	}

}
