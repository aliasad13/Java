package interfaceLearning;

class Pen{

public void write()
{
	System.out.println("Im a pen");
}
}
class Pencil
{
public void write() {
	System.out.println("Im a pencil");
}	
}

class Kit{

public void doSomething(Pencil a) {

a.write();
}


	
	
}
public class Interface {

	public static void main(String[] args) {
                  Pen p = new Pen();
                  Kit k = new Kit();
                  Pencil a = new Pencil();
                  
               k.doSomething(a);
              
	}

}
