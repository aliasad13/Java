package abstractclass;  // another way to print a number


class Print{
	int i;
	public int show() {
		
		return i;         //here we can use return.But if we use it we will have to change void
		
		
	}
}

public class AnotherWayToPrint {

	public static void main(String[] args) {
		Print obj = new Print();
		obj.show(5);
		
		
		
		
		
	}

}
