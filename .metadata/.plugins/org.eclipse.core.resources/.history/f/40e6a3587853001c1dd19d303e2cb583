package com.fibonacci;
//  
//153=1*1*1 +  5*5*5  +  3*3*3
//
public class Revise {

	public static void main(String[] args) {
		
		int n=153;
		int temp=n;
		int sum=0;
		
		while(n>0) {
		
		int r=n%10;
		n=n/10;
		sum=sum + r*r*r;
		}
		if (sum==temp) {
			System.out.println("yes");
		}
	}
		
	
		}
	


