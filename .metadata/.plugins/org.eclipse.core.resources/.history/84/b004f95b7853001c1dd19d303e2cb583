package com.fibonacci;
//  Armstrong
//153=1*1*1 +  5*5*5  +  3*3*3
//palindrome
//12321
public class Revise {

	public static void main(String[] args) {
		
		
		int n=12321;
		int temp =n;
		int rem;int number=0;
		
		while(n>0) {
		rem=n%10;
		n=n/10;
		number = number*10+rem;
		
		}
		
		if(temp==number)
		{
			System.out.println("yes");
		}
		
		
		
	
		}
	


}