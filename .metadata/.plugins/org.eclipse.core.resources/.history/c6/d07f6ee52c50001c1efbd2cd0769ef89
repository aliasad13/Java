package com.accessModifier.anonymous;
class A{

int i=10;int  j =22;

public int result(int i,int j) {
	return i+j;
}
}

//default class only inside package
public class AccessmodiferTest {

	public static void main(String[] args) {
		
		A obj = new A();
		System.out.println(obj.result());
		
	}

}
