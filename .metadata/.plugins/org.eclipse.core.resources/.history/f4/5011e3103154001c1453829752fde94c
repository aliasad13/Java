package varargs1;

class Addition{ 
	public int add(int ... i) {
		int sum = 0;
		for (int q:i) {
		int	sum = sum + q;
		}
		
		return sum;
	}
	
}

public class Var {

	public static void main(String[] args) {
   Addition obj = new Addition();
   System.out.println(obj.add(5,6,4,5,6));
		
		
	}

}
